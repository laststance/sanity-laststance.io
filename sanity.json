{
  "root": true,
  "project": {
    "name": "Laststane-website"
  },
  "api": {
    "projectId": "8qfjmpmv",
    "dataset": "production"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "@sanity/production-preview"
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision"
      ]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "plugins": [
        "@sanity/production-preview"
      ],
      "parts": [
        {
          "implements": "part:@sanity/production-preview/resolve-production-url",
          "path": "./resolveProductionUrl.js"
        }
      ]
    }
  ]
}
